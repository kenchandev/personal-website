{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended",
    "prettier"
  ],
  "plugins": ["import", "jest"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "no-extra-boolean-cast": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "semi": 2
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
