{
  "name": "personal-website",
  "description": "Ken Chan's Personal Website",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --hot --open --useLocalIp",
    "lint": "esw --color 'assets/scripts/**/*.js'",
    "lint:watch": "npm run lint -- --watch",
    "format": "prettier 'assets/scripts/**/*.js'",
    "format:write": "npm run format -- --write ",
    "format:check": "npm run format -- --check ",
    "local:share": "ngrok http 8889 -host-header='localhost:8889'",
    "prebuild": "npm run clean-dist",
    "build": "webpack",
    "build:dev": "NODE_ENV=development npm run build -- --display-error-details",
    "build:prod": "NODE_ENV=production npm run build",
    "psi": "psi https://kenchandev.com",
    "psi:mobile": "npm run psi -- --strategy=mobile",
    "psi:desktop": "npm run psi -- --strategy=desktop",
    "clean-dist": "rimraf ./dist && mkdir ./dist",
    "snyk": "snyk auth && snyk wizard",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist"
  },
  "bugs": {
    "url": "https://github.com/kenchandev/personal-website/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:check && yarn lint"
    }
  },
  "keywords": [
    "website",
    "portfolio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kenchandev/personal-website.git"
  },
  "author": "Ken Chan <kenchandev@gmail.com>",
  "dependencies": {
    "focus-trap": "^5.1.0",
    "focus-visible": "^5.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "cypress": "^3.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-watch": "^4.0.2",
    "file-loader": "^5.0.2",
    "gh-pages": "^1.2.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "ngrok": "^3.1.1",
    "node-sass": "^4.9.3",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^1.19.1",
    "psi": "^4.0.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^0.23.0",
    "svg-icon-inline-loader": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-fix-style-only-entries": "0.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
