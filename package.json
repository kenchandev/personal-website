{
  "name": "personal-website",
  "description": "Ken Chan's Personal Website",
  "scripts": {
    "serve": "serve -l 3000 dist",
    "start": "webpack-dev-server --host 0.0.0.0 --hot --open --useLocalIp",
    "lint": "esw --color 'assets/scripts/**/*.js'",
    "lint:watch": "npm run lint -- --watch",
    "format": "prettier 'assets/scripts/**/*.js'",
    "format:write": "npm run format -- --write",
    "format:check": "npm run format -- --check",
    "local:share": "ngrok http 8889 -host-header='localhost:8889'",
    "prebuild": "npm run clean-dist",
    "build": "webpack",
    "build:dev": "NODE_ENV=development npm run build -- --display-error-details",
    "build:prod": "NODE_ENV=production npm run build",
    "psi": "psi https://kenchandev.com",
    "psi:mobile": "npm run psi -- --strategy=mobile",
    "psi:desktop": "npm run psi -- --strategy=desktop",
    "clean-dist": "rimraf ./dist && mkdir ./dist",
    "snyk": "snyk auth && snyk wizard",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:show-config": "jest --showConfig",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:prod\" \"test:e2e:dev\"",
    "test:e2e:dev": "start-server-and-test start http://localhost:8889 cy:open",
    "test:e2e:prod": "CYPRESS_baseUrl=http://localhost:3000 start-server-and-test serve http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "bugs": {
    "url": "https://github.com/kenchandev/personal-website/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn format:check && yarn lint"
    }
  },
  "lint-staged": {
    "**/*.+(js|json)": [
      "prettier",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "keywords": [
    "website",
    "portfolio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kenchandev/personal-website.git"
  },
  "author": "Ken Chan <kenchandev@gmail.com>",
  "dependencies": {
    "focus-trap": "^5.1.0",
    "focus-visible": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/plugin-transform-strict-mode": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.1",
    "core-js": "^3.6.4",
    "css-loader": "^2.1.1",
    "cypress": "^3.8.2",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-watch": "^4.0.2",
    "file-loader": "^5.0.2",
    "gh-pages": "^1.2.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "is-ci-cli": "^2.0.0",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "ngrok": "^3.1.1",
    "node-sass": "^4.9.3",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^1.19.1",
    "psi": "^4.0.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^0.23.0",
    "svg-icon-inline-loader": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-fix-style-only-entries": "0.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
